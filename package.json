{
  "name": "shaleian-online",
  "version": "0.0.0",
  "author": "Ziphil",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "develop": "run-p develop:client develop:server develop:nodemon",
    "develop:client": "webpack-dev-server --mode development --devtool inline-source-map --config webpack-client.ts --hot",
    "develop:server": "webpack --mode development --config webpack-server.ts --watch",
    "develop:nodemon": "nodemon --inspect -L dist/index.js",
    "build": "run-s build:client build:server",
    "build:client": "webpack --mode production --config webpack-client.ts",
    "build:client:analyze": "cross-env ANALYZE=1 npm run build:client",
    "build:server": "webpack --mode production --config webpack-server.ts",
    "start": "node dist/index.js",
    "lint": "eslint client/**/*.ts client/**/*.tsx server/**/*.ts",
    "lint:fix": "eslint --fix client/**/*.ts client/**/*.tsx server/**/*.ts",
    "command:tsnode": "ts-node -r tsconfig-paths/register",
    "command:tsc": "tsc"
  },
  "engines": {
    "node": "16.x"
  },
  "eslintConfig": {
    "extends": "ziphil",
    "ignorePatterns": "webpack-*.ts",
    "rules": {
      "@typescript-eslint/quotes": "off",
      "prefer-const": [
        "warn",
        {
          "destructuring": "all"
        }
      ]
    }
  },
  "nodemonConfig": {
    "watch": [
      "dist"
    ],
    "ext": "js",
    "ignore": [
      "dist/script/**",
      "dist/static/**",
      "dist/client/**",
      "dist/main*"
    ]
  },
  "dependencies": {
    "@formatjs/intl": "^2.9.0",
    "@formatjs/intl-numberformat": "^5.7.6",
    "@notionhq/client": "^0.4.13",
    "axios": "^0.21.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "css-tree": "^1.1.3",
    "date-fns-timezone": "^0.1.4",
    "discord-api-types": "^0.18.1",
    "discord.js": "^14.13.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "google-auth-library": "^7.1.2",
    "google-spreadsheet": "^3.1.15",
    "googleapis": "^76.0.0",
    "lodash-es": "^4.17.15",
    "mobx": "^5.15.7",
    "mobx-react": "^6.3.1",
    "multer": "^1.4.2",
    "nanoid": "^3.1.23",
    "node-cron": "^3.0.0",
    "query-string": "^6.13.7",
    "react": "^16.14.0",
    "react-css-modules": "^4.7.11",
    "react-dom": "^16.14.0",
    "react-intl": "^5.10.6",
    "react-router-dom": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "soxsot": "0.6.0",
    "talqis": "^0.1.0",
    "ts-essentials": "^7.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@types/axios": "^0.14.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/copy-webpack-plugin": "^6.3.0",
    "@types/cors": "^2.8.10",
    "@types/css-tree": "^1.0.5",
    "@types/express": "^4.17.9",
    "@types/google-spreadsheet": "^3.1.3",
    "@types/html-webpack-plugin": "^3.2.4",
    "@types/loader-utils": "^2.0.2",
    "@types/lodash-es": "^4.17.3",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/multer": "^1.4.6",
    "@types/node-cron": "^2.0.3",
    "@types/react": "^16.14.2",
    "@types/react-css-modules": "^4.6.2",
    "@types/react-dom": "^16.9.10",
    "@types/react-router-dom": "^5.1.6",
    "@types/webpack-bundle-analyzer": "^3.9.0",
    "@types/webpack-node-externals": "^1.7.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/eslint-plugin-tslint": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "copy-webpack-plugin": "^6.4.0",
    "cross-env": "^7.0.3",
    "css-loader": "^3.6.0",
    "eslint": "^8.17.0",
    "eslint-config-ziphil": "^1.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.2",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "json-loader": "^0.5.7",
    "loader-utils": "^2.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^4.0.2",
    "sass-loader": "^12.4.0",
    "source-map-loader": "^1.1.3",
    "style-loader": "^1.3.0",
    "ts-loader": "^8.0.11",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^5.1.6",
    "webpack": "^5.68.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-node-externals": "^3.0.0",
    "yaml-flat-loader": "^1.0.2"
  }
}
